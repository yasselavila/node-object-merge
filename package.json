{
  "name": "yag-object-merge",
  "description": "Tool to merge object recursively",
  "version": "0.0.1",
  "license": "BSD-3-Clause",
  "private": true,
  "author": {
    "name": "Yassel Avila Gil",
    "url": "http://yasselavila.com/"
  },
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/yasselavila/node-object-merge.git"
  },
  "dependencies": {
    "lodash": "^4.15.0",
    "lodash.isarray": "^4.0.0",
    "lodash.isplainobject": "^4.0.6"
  },
  "devDependencies": {
    "typescript": "^2.0.2",
    "@types/lodash": "^4.14.34",
    "yag-node-tools": "^0.0.1"
  },
  "scripts": {
    "start": "npm run build:w",
    "build:pre": "npm run clean && npm run dist:data",
    "build": "npm run build:pre && npm run tsc",
    "build:w": "npm run build:pre && npm run tsc:w",
    "clean": "rm -rf ./dist && mkdir dist",
    "dist:data": "yag-pkg-dist && cp ./README.md ./dist/ && cp ./LICENSE.txt ./dist/",
    "tsc": "tsc --outDir ./dist",
    "tsc:w": "npm run tsc -- -w"
  }
}
